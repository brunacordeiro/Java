import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in); //associação do objeto Scanner ao teclado do tipo console
		String x;
		x = sc.next();

		System.out.println("Ola mundo:" + x);
		
		
		sc.close();
	}

}
