import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in); //associação do objeto Scanner ao teclado do tipo console
		String x;
		int y;
		char z;
		
		x = sc.next(); //String
		y = sc.nextInt();
		z = sc.next().charAt(0);
		
		System.out.println("Ola mundo:" z);
		
		
		sc.close();
	}

}
