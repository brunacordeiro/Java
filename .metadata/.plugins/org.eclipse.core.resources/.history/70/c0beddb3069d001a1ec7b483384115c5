package student;

public class Student {
	
	public String nome;
	public double nota1;
	public double nota2;
	public double nota3;
	
	public double notaFinal(){
		double nota = nota1 + nota2 + nota3;
		double falta = 0;
		
		String result = (nota > 60) ? "PASS" : "Failed";
		
		if (nota < 60){
			System.out.println("Faltam: " + (100 - nota) + "pontos");
		}
	}
}
