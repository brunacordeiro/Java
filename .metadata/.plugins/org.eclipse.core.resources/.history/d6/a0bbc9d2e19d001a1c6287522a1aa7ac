package account;

public class AccountRegistration {
	
	private String name;
	private int account;
	
	//Class Builder
	public AccountRegistration(String name, int account) {
		super();
		this.name = name;
		this.account = account;
	}

	//Get and Set Methods
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAccount() {
		return account;
	}

	/*  - A conta não pode ser alterada
	public void setAccount(int account) {
		this.account = account;
	}
	*/

	@Override
	public String toString() {
		return "name=" + name 
				+ ", account=" + account;
	}
	
	
	
	
}
